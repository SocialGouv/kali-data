{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "target": "es2019",
    "module": "commonjs",
    "strict": false,
    "outDir": "dist"
  },
  "include": ["**/*.d.ts", "**/*.ts"]
}
