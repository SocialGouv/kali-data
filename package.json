{
  "name": "@socialgouv/kali-data__root",
  "version": "1.3.36",
  "license": "Apache-2.0",
  "private": true,
  "homepage": "https://github.com/SocialGouv/kali-data",
  "repository": "https://github.com/SocialGouv/kali-data.git",
  "main": "index.js",
  "files": [
    "data"
  ],
  "scripts": {
    "api:start": "lerna run --parallel --scope \"@socialgouv/kali-data__api\" start",
    "data:check": "lerna run --parallel --scope \"@socialgouv/kali-data__generator\" data:check",
    "data:fetch": "lerna run --parallel --scope \"@socialgouv/kali-data__generator\" data:fetch",
    "data:index": "lerna run --parallel --scope \"@socialgouv/kali-data__generator\" data:index",
    "data:list": "lerna run --parallel --scope \"@socialgouv/kali-data__generator\" data:list",
    "data:update": "lerna run --parallel --scope \"@socialgouv/kali-data__generator\" data:update",
    "dev": "lerna run --parallel --scope \"@socialgouv/kali-data__api\" dev",
    "docker:restart": "yarn docker:stop && docker-compose up --build -d api",
    "docker:start": "docker-compose up -d api",
    "docker:stop": "docker-compose down",
    "setup": "node -r esm ./scripts/dev/setup.js",
    "start": "lerna run --scope \"@socialgouv/kali-data__api\" start",
    "test": "yarn test:lint && yarn test:type && yarn test:unit --silent",
    "test:e2e": "jest --testPathIgnorePatterns=\"<rootDir>/packages/\"",
    "test:lint": "eslint .",
    "test:type": "tsc",
    "test:unit": "jest --testPathIgnorePatterns=\"<rootDir>/e2e/\"",
    "test:update": "jest --testPathIgnorePatterns=\"<rootDir>/e2e/\" --updateSnapshot",
    "test:watch": "yarn test:unit --watch",
    "posttest": "yarn data:check"
  },
  "dependencies": {
    "lerna": "3.22.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-transform-runtime": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@socialgouv/eslint-config-strict": "1.0.0-beta.4",
    "@types/jest": "25.2.3",
    "@types/koa": "2.11.3",
    "@types/node": "14.0.11",
    "axios": "0.19.2",
    "babel-eslint": "10.1.0",
    "eslint": "7.2.0",
    "eslint-plugin-sort-requires": "2.1.0",
    "esm": "3.2.25",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "prettier": "2.0.5",
    "prompt": "1.0.0",
    "semantic-release": "17.0.8",
    "typescript": "3.9.3"
  },
  "engines": {
    "node": ">= 10"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test"
    }
  },
  "jest": {
    "verbose": true,
    "transform": {
      ".+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "release": {
    "branches": [
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "workspaces": [
    "packages/*"
  ]
}
